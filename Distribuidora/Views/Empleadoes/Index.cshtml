@{
    if (Session["Empleado"] != null)
    {
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    else
    {
        Session.Abandon();

        Response.Redirect("~/Empleadoes/Login");
    }

}

<h2>Menu empleados</h2>

<p>
    @Html.ActionLink("Buscar", "Buscar", "Productoes")
</p>
